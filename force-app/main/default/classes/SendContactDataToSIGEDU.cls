public class SendContactDataToSIGEDU {
    
    
    private static Map<Id,Postulaciones__c> mapPostulaciones = new Map<Id,Postulaciones__c>();
    private static Set<Id> contactsToSync = new Set<Id>();

    public static void sendContactsToSIGEDU(){
     
        Map<Id,Postulaciones__c> mapPostulaciones = new Map<Id,Postulaciones__c>();
        map<String,List<FieldMapping__mdt>>  mapping = MetadataUtil.getMapping('Actualizar Contacto SIGEDU');
        List<EnvioWs__c> envios = new List<EnvioWs__c>();
        
        for(Postulaciones__c p : [SELECT Id,Nro_de_orden__c ,Contacto__c FROM Postulaciones__c WHERE Contacto__c in :Trigger.newMap.keySet() AND SendUpdateContactData__c = true]){
            mapPostulaciones.put(p.Contacto__c,p);
        }
      
        for(Contact c:(List<Contact>)Trigger.new){
            if(mapPostulaciones.containsKey(c.id)){
                if(changedData((Contact) Trigger.newMap.get(c.id), (Contact)Trigger.oldMap.get(c.id),mapping.get('SIGEDU'))){
                    envios.add(SenderManager.createEnvio('SenderContact',mapPostulaciones.get(c.Id).Id));
                }
            }
        }
        if(!envios.isEmpty()){
            SenderManager.enqueueEnvio(envios);
        }

    }


    private static Boolean changedData(Contact cNew, Contact cOld,List<FieldMapping__mdt> mapping  ){

         for(FieldMapping__mdt f : mapping){
                    
            if(cNew.get(f.SourceFieldName__c)  != cOld.get(f.SourceFieldName__c)  ){
                return true;
            }

        }
        return false;
    }

