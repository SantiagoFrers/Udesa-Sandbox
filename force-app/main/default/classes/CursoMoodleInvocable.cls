public with sharing class CursoMoodleInvocable {
   

    @InvocableMethod(label='Procesar Cursos Moodle' description='Crea las comisiones relacionadas al curso y las env√≠a a moodle' category= 'Moodle')
    public static void execute (List<Requests> requestList) {
        
        Set<Id> coursesIds = new Set<Id>();

        for (Requests input: requestList) {
            coursesIds.add(input.courseId);
        }

        List<Curso__c> courses =  [SELECT Id, Comisiones__c, Procesado__c,Materias__c  FROM Curso__c WHERE Id in : coursesIds];

        MoodleManager.createCoursesMoodle(courses);
    }

    public class Requests {
        @InvocableVariable(label='Id Curso' description='id del curso a procesar' required=true)
        public Id courseId;
    }

    public class Results {
        @InvocableVariable(label='Records for Output' description='yourDescription' required=true)
        public Boolean procesado;
    }


